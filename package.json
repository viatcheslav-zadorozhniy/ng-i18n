{
  "name": "ng-i18n",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "build:jit": "ng build --localize=false --ts-config=tsconfig-jit.app.json --main=src/main-jit.ts",
    "start": "ng serve",
    "start:he": "ng serve -c=he",
    "start:uk": "ng serve -c=uk",
    "start:jit": "ng serve -c=jit",
    "extract-i18n:arb": "ng extract-i18n -c=arb",
    "extract-i18n:json": "ng extract-i18n -c=json",
    "extract-i18n:xlf": "ng extract-i18n -c=xlf",
    "extract-i18n:xlf2": "ng extract-i18n -c=xlf2",
    "extract-i18n:xmb": "ng extract-i18n -c=xmb",
    "extract-i18n-merge:xlf": "ng run app:extract-i18n-merge:xlf",
    "extract-i18n-merge:xlf2": "ng run app:extract-i18n-merge:xlf2"
  },
  "dependencies": {
    "@angular/animations": "^15.2.0",
    "@angular/common": "^15.2.0",
    "@angular/compiler": "^15.2.0",
    "@angular/core": "^15.2.0",
    "@angular/forms": "^15.2.0",
    "@angular/platform-browser": "^15.2.0",
    "@angular/platform-browser-dynamic": "^15.2.0",
    "@angular/router": "^15.2.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.4",
    "@angular/cli": "~15.2.4",
    "@angular/compiler-cli": "^15.2.0",
    "@angular/localize": "15.2.3",
    "ng-extract-i18n-merge": "2.6.0",
    "typescript": "~4.9.4"
  }
}
